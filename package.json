{
  "name": "havit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-setup-port": "adb reverse tcp:8081 tcp:8080",
    "test": "mocha --compilers js:babel-core/register --watch tests/*.js",
    "start": "/usr/local/bin/react-native start",
    "bundle": "/usr/local/bin/react-native bundle --dev false --assets-dest ./ --entry-file index.ios.js --platform ios --minify --bundle-output ios/main.jsbundle",
    "clean-babelrc-files": "cd node_modules; find . -name .babelrc | grep -v packager | xargs rm",
    "release": "/usr/local/bin/react-native bundle --dev false --assets-dest ./ --entry-file index.ios.js --platform ios --minify --bundle-output ios/main.jsbundle; code-push release havit-ios ios/main.jsbundle 1.0.0"
  },
  "dependencies": {
    "gl-react": "^2.0.9",
    "gl-react-native": "^2.17.7",
    "lodash": "^3.10.1",
    "react-native": "0.18.0-rc",
    "react-native-accordion": "^0.2.2",
    "react-native-code-push": "^1.5.3-beta",
    "react-native-icons": "^0.7.0",
    "react-native-linear-gradient": "^1.3.0",
    "react-native-material-kit": "^0.2.4",
    "react-native-router-flux": "^2.0.5",
    "react-native-scrollable-tab-view": "^0.3.3",
    "react-redux": "^3.1.2",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.0",
    "redux-logger": "^2.3.1",
    "redux-promise": "^0.5.0",
    "redux-storage": "^1.4.1",
    "redux-thunk": "^1.0.1"
  },
  "devDependencies": {
    "deep-freeze": "0.0.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.2",
    "babel-eslint": "^4.1.6",
    "expect": "^1.13.4",
    "mocha": "^2.3.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gr4yscale/havit"
  }
}
